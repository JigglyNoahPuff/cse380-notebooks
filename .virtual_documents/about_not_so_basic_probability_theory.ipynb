def compute_Eulers_famous_sum(max):
  return sum((1.0 / (n * n) for n in range(1, max + 1)))


print(list(map(compute_Eulers_famous_sum, range(1, 100))))


print(list(map(compute_Eulers_famous_sum, range(99997, 100000))))


